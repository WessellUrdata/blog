{
  "name": "blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/mdx": "^2.3.1",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "dayjs": "^1.11.11",
    "katex": "^0.16.10",
    "rehype-katex": "^7.0.0",
    "remark-emoji": "^4.0.1",
    "remark-math": "^6.0.0",
    "to-jyutping": "^1.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/tailwind": "^5.1.0",
    "@tailwindcss/typography": "^0.5.13",
    "astro": "^4.11.3",
    "daisyui": "^4.12.10",
    "prettier-plugin-md-nocjsp": "^1.5.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2"
  },
  "prettier": {
    "htmlWhitespaceSensitivity": "ignore",
    "bracketSameLine": true,
    "overrides": [
      {
        "files": [
          "*.md",
          "README"
        ],
        "options": {
          "parser": "markdown-nocjsp"
        }
      },
      {
        "files": [
          "*.mdx"
        ],
        "options": {
          "parser": "mdx-nocjsp"
        }
      }
    ]
  }
}
